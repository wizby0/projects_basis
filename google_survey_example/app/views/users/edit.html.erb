<h1>Editing user</h1>


<%= form_for :user, url: user_path(@user), method: :patch do |f| %>
 
<p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :nickname %><br>
    <%= f.text_field :nickname %>
  </p>

  <p>
     <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>

<p>
<%= f.submit %>
</p>

<% end %>

<%= link_to 'Back', users_path %>